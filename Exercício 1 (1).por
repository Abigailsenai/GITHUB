programa {
  funcao inicio() {
    /*Leia dois números, e mostre a soma, subtração, divisão e multiplicação
    */
   real num1
   real num2

   escreva ("Escreva um número")
   leia (num1)
   escreva ("\n")

   escreva ("Escreva outro número")
   leia (num2)
   escreva ("\n")

   escreva (num1 + num2)
   escreva ("\n")

   escreva (num1 - num2)
   escreva ("\n")

   escreva (num1 / num2)
   escreva ("\n")

   escreva (num1 * num2)
  }
}
